<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>AppStore-Server</display-name>
  <servlet>
    <servlet-name>hello</servlet-name>
    <servlet-class>com.app_store.Test</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>hello</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>appstore</servlet-name>
    <servlet-class>com.appstore.servlet.AppStoreController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>appstore</servlet-name>
    <url-pattern>/App_store</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>download</servlet-name>
    <servlet-class>com.appstore.servlet.DownloadApkController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>download</servlet-name>
    <url-pattern>/download</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>count</servlet-name>
  	<servlet-class>com.appstore.servlet.DownloadCountCul</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>count</servlet-name>
  	<url-pattern>/count</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>urlfilter</filter-name>
    <filter-class>com.appstore.filter.UrlFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>urlfilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
</web-app>